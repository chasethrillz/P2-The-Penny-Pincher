      <div class="container">
        <div class="row">
            <div class="col s12">
              <div class="card N/A transparent hoverable editor">
                <div class="card-content black-text">
                  <span class="card-title">Add New Item</span>
                  <form class="add">
                  <div class="row">
                    <form class="col s12">
                        <div class="input-field col s6">
                          <input id="add-name" type="text" class="validate">
                          <label for="add-name">Name:</label>
                        </div>
                        <div class="input-field col s6">
                          <input id="currency" type="text" class="validate">
                          <label for="currency">Amount:</label>
                        </div>


                  <div class="row">
                    
                      <div class="col s3">
                      <div class="input-field">
                        <input id="category-field" type="text" class="validate ctrl-btn">
                        <label for="category">Category</label>
                        <span class="helper-text" data-error="wrong" data-success="ready"></span>
                      </div>
                      </div>
                    
                  
                    <div class="col s3">
                      <div class="input-field">
                        <input id="priority-field" type="text" class="validate ctrl-btn">
                        <label for="priority">Priority</label>
                        <span class="helper-text" data-error="wrong" data-success="ready"></span>
                      </div>
                    </div>
                  </div>
            
                  
                </div>

                  <div class="row">

                  <div class="col s3 ">
                  <!-- Dropdown Trigger -->
                  <a class='dropdown-trigger btn' href='#' data-target='categories'>Categories</a>
                  
                  <!-- Dropdown Structure -->
                  <ul id='categories' class='dropdown-content'>
                  
                    {{#each categories}}
                  
                    <li>
                      <a href="#!" class="cat">{{this.dataValues.category}}</a>
                    </li>
                    <li class="divider" tabindex="-1"></li>
                  
                    {{/each}}
                  
                  </ul>
                  </div>
                  <div class="col s3 ">
                  <!-- Dropdown Trigger -->
                  <a class='dropdown-trigger btn' href='#' data-target='priority'>Priority</a>
                  
                  <!-- Dropdown Structure -->
                  <ul id='priority' class='dropdown-content'>
                    <li><a class="pri" href="#!">High</a></li>
                    <li class="divider" tabindex="-1"></li>
                    <li><a class="pri" href="#!">Medium</a></li>
                    <li class="divider" tabindex="-1"></li>
                    <li><a class="pri" href="#!">Low</a></li>
                    <li class="divider" tabindex="-1"></li>           
                  </ul>
                </div>
               
                
                <button class="btn waves-effect waves-light btn-large #000000 black right" type="submit" name="add-item">Add
                  <i class="material-icons right">send</i>
                </button>
                </div>
                </form>
                </div>
                </div>
              </div>
            </div>
          </div>
      </div>
        </div>

        <div class="container table">
          <table class="striped responsive-table">
            <thead>
              <tr>
                  <th>Date</th>
                  <th>Name</th>
                  <th>Catagory</th>
                  <th>Proiority</th>
                  <th>Amount</th>
              </tr>
            </thead>
    
            <tbody>
              <tr>
                <td>Alvin</td>
                <td>some date</td>
                <td>Eclair</td>
                <td>High</td>
                <td>$0.87</td>
              </tr>
              <tr>
                <td>Alan</td>
                <td>some date</td>
                <td>Jellybean</td>
                <td>Medium</td>
                <td>$3.76</td>
              </tr>
              <tr>
                <td>Jonathan</td>
                <td>some date</td>
                <td>Lollipop</td>
                <td>Low</td>
                <td>$7.00</td>
              </tr>
            </tbody>
          </table>
        </div>
                      
  
                      
             
      
    
